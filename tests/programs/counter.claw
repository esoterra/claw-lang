let mut counter-s32: s32 = 0;

export func increment-s32() -> s32 {
    counter-s32 = counter-s32 + 1;
    return counter-s32;
}

export func decrement-s32() -> s32 {
    counter-s32 = counter-s32 - 1;
    return counter-s32;
}

let mut counter-s64: s64 = 0;

export func increment-s64() -> s64 {
    counter-s64 = counter-s64 + 1;
    return counter-s64;
}

export func decrement-s64() -> s64 {
    counter-s64 = counter-s64 - 1;
    return counter-s64;
}
