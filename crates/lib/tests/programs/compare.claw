export func min-u32(left: u32, right: u32) -> u32 {
    if left < right {
        return left;
    }
    return right;
}

export func max-u32(left: u32, right: u32) -> u32 {
    if left > right {
        return left;
    }
    return right;
}

export func min-u64(left: u64, right: u64) -> u64 {
    if left < right {
        return left;
    }
    return right;
}

export func max-u64(left: u64, right: u64) -> u64 {
    if left > right {
        return left;
    }
    return right;
}