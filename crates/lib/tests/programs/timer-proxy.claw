import { now } from wasi:clocks/monotonic-clock;
import { level, log } from wasi:logging/logging;

import foo: func(a: string) -> string;

let max-millis: u64 = 1000;

export func bar(a: string) -> string {
    let start = now();
    let res = foo(a);
    let end = now();
    if end - start > max-millis {
        log(level::warning, "profiling", "Calling foo took a long time");
    }
    return res;
}
